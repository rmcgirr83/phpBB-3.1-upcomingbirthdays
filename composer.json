{
    "name": "rmcgirr83/upcomingbirthdays",
    "type": "phpbb-extension",
    "description": "Shows upcoming birthdays on index page, number of days can be modified via ACP.",
    "homepage": "https://github.com/rmcgirr83/upcomingbirthdays",
    "version": "1.0.11",
    "time": "2024-07-17",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Rich McGirr",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "phpbb/phpbb": ">=3.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Upcoming Birthday List",
        "soft-require": {
            "phpbb/phpbb": ">=3.3"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/upcoming_birthday_list",
            "filename": "version_check",
            "ssl": true
        }
    }
}